    "# userid@domainname.ext\n",
    "\n",
    "import re\n",
    "\n",
    "pattern = '^[A-Za-z0-9._%+-]+@[A-Za-z0-9._%+-]+\\.[a-zA-Z]{2,}$'\n",
    "email_id = input('Enter your email ID:')\n",
    "email = re.search(pattern, email_id)\n",
    "\n",
    "if email:\n",
    "    print(f'{email_id} is valid')\n",
    "else:\n",
    "    print(f'{email_id} is Invalid.')"
